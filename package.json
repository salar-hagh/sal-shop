{
  "name": "sal-shop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:dev": "env-cmd -f dev.env npm run dev",
    "start:stage": "env-cmd -f stage.env npm run dev",
    "start:prod": "env-cmd -f prod.env npm run dev",
    "build:dev": "env-cmd -f dev.env npm run build",
    "build:stage": "env-cmd -f stage.env npm run build",
    "build:prod": "env-cmd -f prod.env npm run build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.35.1",
    "axios": "^1.6.8",
    "env-cmd": "^10.1.0",
    "formik": "^2.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-tooltip": "^5.26.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
